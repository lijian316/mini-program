					<view>{{item.name}}</view>
				</view>
			</van-col>
		</van-row>
	</van-popup>

	<!-- 我提供的编辑 -->
	<van-popup show="{{ giveShow }}" round position="bottom" custom-style="height: 80%" bind:close="onClose" custom-class="pop">
